{
  "compilerOptions": {
    "target": "ESNext",
    "baseUrl": "./src",
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "types": ["vitest"]
  }
}
